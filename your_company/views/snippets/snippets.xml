<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">YourCompany</div>
                <div class="o_panel_body">
                    <t t-snippet="your_company.compony_leaders" t-thumbnail="/your_company/static/src/img/snippets/agents.svg">
                        <keywords>company, leaders</keywords>
                    </t>
               
                    <t t-snippet="your_company.explore_employees" t-thumbnail="/your_company/static/src/img/snippets/explore-employees.svg">
                        <keywords>explore, Employees</keywords>
                    </t>
                </div>
            </div>
        </xpath>
    </template>

   
</odoo>