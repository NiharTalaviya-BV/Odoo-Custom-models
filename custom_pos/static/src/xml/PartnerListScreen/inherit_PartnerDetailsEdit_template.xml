<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="InheritPartnerDetailsEdit" t-inherit="point_of_sale.PartnerDetailsEdit" t-inherit-mode="extension" owl="1">
      
           <xpath expr="//div[hasclass('partner-details-right')]" position="inside">
            <div class="partner-detail">
                    <span class="label">Alt-Phone</span>
                    <input class="detail" name="alternate_phone_num" t-model="changes.alternate_phone_num" t-on-change="captureChange" placeholder="Alt-Phone Number" />
            </div>
            
        </xpath>
    </t>
</templates>